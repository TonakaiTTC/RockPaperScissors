import random
choices = ['Rock', 'Paper', 'Scissors' , 'rock', 'paper', 'scissors']
compindex = random.randint(0,2)
compchoice = choices[compindex]
userChoice = input('Select rock, paper, or scissors ')

if compchoice == 'Paper':
    if userChoice == 'Paper':
        print('Computer chose ' + compchoice)
        print('Dang it we tied!')
